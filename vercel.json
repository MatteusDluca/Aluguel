{
    "version": 2,
    "builds": [
        {
            "src": "dist/main.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/register",
            "method" : "POST",
            "dest": "/dist/main.js"
        },
        {
            "src": "/login",
            "method" : "POST",
            "dest": "/dist/main.js"
        },
        {
            "src": "/stock",
            "method" : "POST, GET, DELETE, PATCH",
            "dest": "/dist/main.js"
        },
        {
            "src": "/(.*)",
            "dest": "/dist/main.js"
        }
    ]
}